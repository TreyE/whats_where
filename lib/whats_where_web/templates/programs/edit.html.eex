<h1>Edit Program</h1>
<div class="row">
<div class="col-sm-12">
<%= form_for @form, Routes.programs_path(@conn, :update, @id), [as: :program], fn f -> %>
<div class="form-group">
  <%= label f, :name, "Name" %>
  <%= text_input f, :name, class: "form-control" %>
  <%= error_tag @form, :name %>
</div>
<%= submit "Submit", class: "btn btn-primary" %>
<% end %>
</div>
</div>